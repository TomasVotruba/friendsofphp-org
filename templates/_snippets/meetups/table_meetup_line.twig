{# @var meetup \Fop\Meetup\ValueObject\Meetup #}
<tr class="meetup" data-longitude="{{ meetup.longitude }}" data-latitude="{{ meetup.latitude}}" data-datetime="{{ meetup.startDateTime|date("Y-m-d H:i") }}">
    <td class="w-50">
        <strong>
            <a href="{{ meetup.url }}">{{ meetup.name }}</a>
        </strong>

        <br>

        <small class="text-secondary">{{ meetup.userGroup }}</small>
    </td>

    <td class="w-20">

        <a href="#" onclick="relocate({{ meetup.latitude }}, {{ meetup.longitude }})">
            {{ meetup.city }}
        </a>

        <br>

        <small class="text-secondary">{{ meetup.country }}</small>

    </td>

    <td class="w-30">
        <span class="text-nowrap text-{% if meetup.startInDays < 7 %}success{% elseif meetup.startInDays < 14 %}warning{% else %}danger{% endif %}">
            {{ meetup.startDateTime|date("D") }},
            <strong>{{ meetup.startDateTime|date("M, d") }}</strong>
        </span>
        <br>
        <small class="text-secondary">{{ meetup.startInDays }} days from now at {{ meetup.startDateTime|date("H:i") }}</small>

    </td>
    </tr>
